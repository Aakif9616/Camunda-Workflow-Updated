      <bpmndi:BPMNShape id="HRReviewTask_di" bpmnElement="HRReviewTask">
        <dc:Bounds x="950" y="180" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- HR Decision Gateway -->
      <bpmndi:BPMNShape id="HRDecisionGateway_di" bpmnElement="HRDecisionGateway" isMarkerVisible="true">
