          <dc:Bounds x="1092" y="165" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Parallel Review Gateway -->
      <bpmndi:BPMNShape id="ParallelReviewGateway_di" bpmnElement="ParallelReviewGateway">
        <dc:Bounds x="1205" y="195" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1194" y="165" width="72" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Team Lead Review Task -->
      <bpmndi:BPMNShape id="TeamLeadReviewTask_di" bpmnElement="TeamLeadReviewTask">
        <dc:Bounds x="1310" y="120" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Project Manager Review Task -->
      <bpmndi:BPMNShape id="ProjectManagerReviewTask_di" bpmnElement="ProjectManagerReviewTask">
        <dc:Bounds x="1310" y="250" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Parallel Join Gateway -->
      <bpmndi:BPMNShape id="ParallelJoinGateway_di" bpmnElement="ParallelJoinGateway">
        <dc:Bounds x="1465" y="195" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1456" y="165" width="68" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- TL/PM Decision Gateway -->
      <bpmndi:BPMNShape id="TLPMDecisionGateway_di" bpmnElement="TLPMDecisionGateway" isMarkerVisible="true">
        <dc:Bounds x="1565" y="195" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1548" y="165" width="84" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Head HR Review Task -->
      <bpmndi:BPMNShape id="HeadHRReviewTask_di" bpmnElement="HeadHRReviewTask">
        <dc:Bounds x="1670" y="180" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Head HR Decision Gateway -->
      <bpmndi:BPMNShape id="HeadHRDecisionGateway_di" bpmnElement="HeadHRDecisionGateway" isMarkerVisible="true">
        <dc:Bounds x="1825" y="195" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1808" y="165" width="84" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Company Manager Review Task -->
      <bpmndi:BPMNShape id="CompanyManagerReviewTask_di" bpmnElement="CompanyManagerReviewTask">
        <dc:Bounds x="1930" y="80" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Company Manager Decision Gateway -->
      <bpmndi:BPMNShape id="CompanyManagerDecisionGateway_di" bpmnElement="CompanyManagerDecisionGateway" isMarkerVisible="true">
        <dc:Bounds x="2085" y="95" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="2068" y="65" width="84" height="27" />
        </bpmndi:BPMNLabel>
